{
  "$schema": "bp://types/botpress.config.schema.json",
  "httpServer": {
    "host": "localhost",
    "port": 3000,
    "backlog": 0,
    "bodyLimit": "10mb",
    "cors": {
      "enabled": true
    },
    "externalUrl": "",
    "session": {
      "enabled": false,
      "maxAge": "1h"
    },
    "socketTransports": [
      "websocket",
      "polling"
    ],
    "rateLimit": {
      "enabled": false,
      "limitWindow": "30s",
      "limit": 600
    },
    "headers": {
      "X-Powered-By": "Botpress"
    }
  },
  "dialog": {
    "janitorInterval": "10s",
    "timeoutInterval": "2m",
    "sessionTimeoutInterval": "30m"
  },
  "logs": {
    "dbOutput": {
      "expiration": "2 weeks",
      "janitorInterval": "30s"
    },
    "fileOutput": {
      "enabled": false,
      "folder": "./",
      "maxFileSize": 10000
    }
  },
  "modules": [
